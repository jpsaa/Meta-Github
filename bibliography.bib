	keywords = {white matter, Brain Ischemia, Cerebral Cortex, Cognition, Memory, Stroke, Ischemic Attack, Transient, Cross-Sectional Studies, Executive Function, Middle Aged, Aged, Female, Magnetic Resonance Imaging, cognition, atrial fibrillation, Humans, Neuropsychological Tests, Male, Risk Factors, stroke, magnetic resonance imaging},
	file = {Submitted Version:/Users/Womps/Zotero/storage/552LPXYT/Wang et al. - 2016 - Risk Factors and Cognitive Relevance of Cortical C.pdf:application/pdf}
