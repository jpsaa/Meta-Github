@article{geng_midterm_2017,
	title = {Midterm blood pressure variability is associated with poststroke cognitive impairment: a prospective cohort study},
	volume = {8},
	issn = {1664-2295},
	doi = {10.3389/fneur.2017.00365},
	shorttitle = {Midterm Blood Pressure Variability Is Associated with Poststroke Cognitive Impairment},
	abstract = {{OBJECTIVE}: The aim of this study was to investigate the relationship between blood pressure variability ({BPV}) and poststroke cognitive impairment ({PSCI}).
{METHODS}: Seven-hundred ninety-six patients with acute ischemic stroke were included in this study. Midterm {BPV} was evaluated by calculating the {SD} and coefficient of variation ({CV}, 100‚Äâ√ó‚Äâ{SD}/mean) of systolic blood pressure ({SBP}) and diastolic blood pressure during the 7‚Äâdays after stroke onset. Cognitive function was assessed using the Montreal Cognitive Assessment ({MoCA}) at admission and at all follow-up visits. Patients with {MoCA} scores {\textless}26 were considered to have {PSCI}.
{RESULTS}: The incidence of {PSCI} reached its peak (72\%) 3‚Äâmonths after stroke onset and decreased to 30.3\% at 12‚Äâmonths poststroke. After adjusting for covariables, the increase in the prevalence of {PSCI} at 3‚Äâmonths was independently associated with increases in the {CV} of blood pressure during the 7‚Äâdays after stroke [odds ratios and 95\% {CI} for patients in the second to fifth quintiles of {SBP} {CV} were 2.28 (1.18, 4.39), 2.33 (1.18, 4.62), 2.69 (1.31, 5.53), and 4.76 (1.95, 11.67), respectively]. Sub-analysis of the {MoCA} scores revealed that the patients had impairments in visuoperceptual abilities and executive functions, as well as in naming and delayed recall (p‚Äâ{\textless}‚Äâ0.05).
{CONCLUSION}: Midterm {BPV} during the early phase of acute ischemic stroke is independently associated with {PSCI}, especially in the visuoperceptual, executive, and delayed recall domains.
{CLINICAL} {TRIAL} {REGISTRATION}: http://www.chictr.org.cn, identifier {ChiCTR}-{TRC}-14004804.},
