	date = {2015-04},
	pmid = {25677599},
	keywords = {Stroke, Ischemic Attack, Transient, Cognition Disorders, Risk, Middle Aged, Aged, Female, Postural Balance, Aged, 80 and over, Gait, gait, Humans, Follow-Up Studies, Male, stroke, mild cognitive impairment, Biomarkers},
