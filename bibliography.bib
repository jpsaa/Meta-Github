	url = {https://www.strokejournal.org/article/S1052-3057(16)30217-8/abstract},
	doi = {10.1016/j.jstrokecerebrovasdis.2016.07.027},
	shorttitle = {Dementia after Three Months and One Year from Stroke},
	abstract = {{\textless}h3{\textgreater}Objective{\textless}/h3{\textgreater}{\textless}p{\textgreater}Stroke is an important risk factor for dementia, but the exact mechanism involved in cognitive decline remains unclear.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}Patients were divided into 2 groups: poststroke dementia group ({PSD}) and poststroke nondementia group ({PSND}). Variables and neuroradiological hallmarks were compared between 2 groups at 3 months (114 subjects) and 1 year (105 subjects) after stroke.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}Older age ({OR} 1.11, 95\% {CI} 1.0-1.2; \textit{P} {\textless} .05), education ({OR} .6, 95\% {CI} .4-.8; \textit{P} {\textless} .05), prestroke {IQCODE} (Informant Questionnaire on Cognitive Decline in the Elderly; {OR} .78, 95\% {CI} .1-5.9; \textit{P} {\textless} .05), premorbid apathy ({OR} 2.03, 95\% {CI} 1.1-3.7; \textit{P} {\textless} .05), and medial temporal lobe atrophy ({MTLA}) ({OR} 6.14, 95\% {CI} 1.4-26.2; \textit{P} {\textless} .05) were independently associated with {PSD} at 3 months after a cerebrovascular event, whereas at 1-year follow-up older age ({OR} 1.1, 95\% {CI} 1.0-1.2; \textit{P} {\textless} .05), prestroke {IQCODE} ({OR} .05, 95\% {CI} .0-.9; \textit{P} {\textless} .05), {MTLA} ({OR} 1.3, 95\% {CI} 1.0-1.6; \textit{P} {\textless} .05), and {APACHE} {II} (Acute Physiology and Chronic Health Evaluation; {OR} .6, 95\% {CI} .4-.9; \textit{P} {\textless} .05) were independently associated with {PSD}.{\textless}/p{\textgreater}{\textless}h3{\textgreater}Conclusions{\textless}/h3{\textgreater}{\textless}p{\textgreater}Acute cerebrovascular disease could not be the only one mechanism explaining {PSD}. Neurodegenerative pathology must be taken into account.{\textless}/p{\textgreater}},
	pages = {2735--2745},
	number = {11},
	journaltitle = {Journal of Stroke and Cerebrovascular Diseases},
	shortjournal = {J Stroke Cerebrovasc Dis},
	author = {Caratozzolo, Salvatore and Mombelli, Giulia and Riva, Maddalena and Zanetti, Marina and Gottardi, Federica and Padovani, Alessandro and Rozzini, Luca},
