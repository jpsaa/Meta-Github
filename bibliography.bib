	keywords = {Depressive Disorder, Dementia, Disabled Persons, Stroke, Patient Discharge, Aged, Female, Physical Therapy Modalities, Thailand, Patient Education as Topic, Psychiatric Status Rating Scales, Activities of Daily Living, Humans, Neuropsychological Tests, Follow-Up Studies, Treatment Outcome, Male, Program Evaluation, Stroke Rehabilitation, House Calls}
}

@article{chang_neural_2011,
	title = {Neural correlates of donepezil-induced cognitive improvement in patients with right hemisphere stroke: a pilot study},
	volume = {21},
	issn = {1464-0694},
	doi = {10.1080/09602011.2011.582708},
	shorttitle = {Neural correlates of donepezil-induced cognitive improvement in patients with right hemisphere stroke},
	abstract = {Donepezil has been proven effective in the treatment of Alzheimer's disease and vascular dementia. However, its effects on the cognitive neural network have not been fully investigated. The purpose of this study was to evaluate the effect of donepezil on reorganisation of the cognitive neural network in patients with post-stroke cognitive impairment using functional {MRI} ({fMRI}). Fourteen patients with stroke in the right hemisphere were enrolled. Participants were randomly assigned to the experimental or the control group. Donepezil (5 mg) or placebo was administered daily for four weeks. Cognitive function assessment was performed before and immediately after treatment, and repeated one month after cessation of treatment. {fMRI} was performed before and after treatment. Ten out of 14 patients (six in the experimental group, four in the control group) successfully completed all experimental processes. The experimental group showed significant improvements in the Mini-Mental Status Examination during the post-treatment evaluation and one-month follow-up compared to the pre-treatment evaluation (p {\textless} .05). No improvement was observed in the control group. In the experimental group {fMRI} showed increased activation in both prefrontal areas, both inferior frontal lobes, and in the left inferior parietal lobe. Increased recruitment of the parieto-frontal networks in the selected patients was considered to be a neural correlate of cognitive improvement induced by donepezil.},
	pages = {502--514},
	number = {4},
	journaltitle = {Neuropsychological Rehabilitation},
	shortjournal = {Neuropsychol Rehabil},
	author = {Chang, Won Hyuk and Park, Yun H. and Ohn, Suk Hoon and Park, Chang-hyun and Lee, Peter K. W. and Kim, Yun-Hee},
	date = {2011-08},
	pmid = {21714757},
	keywords = {Indans, Piperidines, Neural Pathways, Stroke, Brain Mapping, Cognition Disorders, Middle Aged, Aged, Female, Pilot Projects, Adult, Functional Laterality, Humans, Neuropsychological Tests, Male, Donepezil, Nootropic Agents}
}

@article{delbari_effect_2011,
	title = {Effect of methylphenidate and/or levodopa combined with physiotherapy on mood and cognition after stroke: a randomized, double-blind, placebo-controlled trial},
