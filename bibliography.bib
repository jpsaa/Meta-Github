	number = {1},
	journaltitle = {{PLoS} One},
	shortjournal = {{PLoS} {ONE}},
	author = {Bath, Philip M. and Scutt, Polly and Blackburn, Daniel J. and Ankolekar, Sandeep and Krishnan, Kailash and Ballard, Clive and Burns, Alistair and Mant, Jonathan and Passmore, Peter and Pocock, Stuart and Reckless, John and Sprigg, Nikola and Stewart, Rob and Wardlaw, Joanna M. and Ford, Gary A. and {PODCAST Trial Investigators}},
	date = {2017},
