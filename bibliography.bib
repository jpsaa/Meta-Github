	date = {2017},
	pmid = {28626050},
	keywords = {dementia, Cerebral Cortex, Stroke, Cognition Disorders, Middle Aged, Aged, Diffusion Magnetic Resonance Imaging, Female, Prospective Studies, Patient Admission, Humans, Follow-Up Studies, Male, stroke, basal ganglia, cognition disorders, magnetic resonance imaging, Organ Size}
