	number = {4},
	journaltitle = {European Journal of Physical and Rehabilitation Medicine},
	shortjournal = {Eur J Phys Rehabil Med},
	author = {Rozental-Iluz, Clara and Zeilig, Gabi and Weingarden, Harold and Rand, Debbie},
