@article{bohannon_functional_2003,
	title = {Functional gains during acute hospitalization for stroke},
	volume = {17},
	issn = {1545-9683},
	doi = {10.1177/0888439003256045},
	abstract = {{OBJECTIVES}: 1) Confirm clinimetric suitability of 5 Functional Independence Measure ({FIM}) items for characterizing functional independence. 2) Describe functional changes and relationships between selected variables and functional independence at discharge and functional change.
{METHODS}: For 451 patients hospitalized for acute stroke, scores were recorded at admission and discharge for 5 {FIM} items (transfers, eating, walking, expression, and memory). The sum of the 5 {FIM} items and functional change between admission and discharge were calculated.
{RESULTS}: The {FIM} scores were internally consistent (alpha = .894 and .918) and reflected 1 factor. Functional independence increased (t = 17.46, P {\textless} .001) over a mean 5.5 days. Regression analysis showed the same variables (admission function, {NIHSS} score, length of stay, age, and total therapy units) to best predict discharge function (R2 = .804) and function change (R2 = .184).
{CONCLUSIONS}: Together, 5 easily observed {FIM} items demonstrated that functional independence increased during hospitalization. Discharge function and functional change were predicted significantly (but weakly) by therapy units.},
