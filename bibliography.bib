	pmcid = {PMC2996293},
	keywords = {Adult, Auditory Cortex, Brain Injuries, Cognition, Female, Follow-Up Studies, Humans, Magnetoencephalography, Male, Memory, Short-Term, Middle Aged, Middle Cerebral Artery, Music Therapy, Neuropsychological Tests, Neuropsychology, Perceptual Disorders, Stroke, Stroke Rehabilitation},
