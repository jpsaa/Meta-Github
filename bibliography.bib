	number = {3},
	journaltitle = {International Journal of Stroke: Official Journal of the International Stroke Society},
	shortjournal = {Int J Stroke},
	author = {Sivakumar, Leka and Riaz, Parnian and Kate, Mahesh and Jeerakathil, Thomas and Beaulieu, Christian and Buck, Brian and Camicioli, Richard and Butcher, Ken},
	date = {2017},
	pmid = {27784823},
	keywords = {White Matter, Brain, Stroke, Ischemic Attack, Transient, Time Factors, leukoaraiosis, Severity of Illness Index, Middle Aged, Aged, Linear Models, Diffusion Magnetic Resonance Imaging, Female, Prospective Studies, Humans, Neuropsychological Tests, Male, ischemic stroke, Cognitive Dysfunction, magnetic resonance imaging, Organ Size, lesions}
