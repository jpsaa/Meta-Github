	number = {7},
	journaltitle = {International Journal of Stroke: Official Journal of the International Stroke Society},
	shortjournal = {Int J Stroke},
	author = {Ihle-Hansen, Hege and Thommessen, Bente and Fagerland, Morten W. and √òkseng√•rd, Anne R. and Wyller, Torgeir B. and Engedal, Knut and Fure, Brynjar},
	date = {2014-10},
	pmid = {23205666},
	keywords = {Dementia, cognitive impairment, Stroke, Ischemic Attack, Transient, Aged, Incidence, Female, Secondary Prevention, secondary prevention, Humans, Neuropsychological Tests, Follow-Up Studies, Treatment Outcome, Male, Risk Factors, stroke, Cognitive Dysfunction, dementia after stroke, risk factor management}
