	author = {Lee, Kyoung Bo and Lim, Seong Hoon and Kim, Kyung Hoon and Kim, Ki Jeon and Kim, Yang Rae and Chang, Woo Nam and Yeom, Jun Woo and Kim, Young Dong and Hwang, Byong Yong},
	date = {2015-06},
	pmid = {25603539},
