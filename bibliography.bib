	keywords = {Stroke, Walking, conventional physiotherapy, robotic rehabilitation, Exercise Therapy, Patient Discharge, Middle Aged, Aged, Female, Robotics, Physical Therapy Modalities, Postural Balance, Gait, Hemiplegia, Humans, Treatment Outcome, Male, stroke, Retrospective Studies, Stroke Rehabilitation},
