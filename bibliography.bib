	number = {12},
	journaltitle = {{PLoS} One},
	shortjournal = {{PLoS} {ONE}},
	author = {S√§rk√§m√∂, Teppo and Tervaniemi, Mari and Soinila, Seppo and Autti, Taina and Silvennoinen, Heli M. and Laine, Matti and Hietanen, Marja and Pihko, Elina},
	date = {2010-12-02},
