@article{turunen_executive_2016,
	title = {Executive Impairment Is Associated with Impaired Memory Performance in Working-Aged Stroke Patients},
	volume = {22},
	issn = {1469-7661},
	doi = {10.1017/S1355617716000205},
	abstract = {{OBJECTIVES}: Executive dysfunction is associated with impaired memory performance, but controversies remain about which aspects of memory are involved and how general intelligence influences these connections. We aimed to clarify these connections in stroke patients by comparing various memory measures in patients with and without executive impairment.
{METHODS}: Our consecutive cohort included patients with a first-ever ischemic stroke. Neuropsychological assessments were completed 6 months and 2 years after stroke. We classified patients as executively impaired, when at least two of five executive measures were defective at 6 months. At both 6 months and 2 years, we compared list learning of unrelated words, story recall, and recall of geometric figures in patients with and without executive impairment, while controlling for general intelligence.
{RESULTS}: Patients with executive impairment (n=66; 37\%) performed worse in list learning (p=.001; partial Œ∑2=.058) and immediate recall of a logical passage (p=.010; partial Œ∑2=.037) 6 months after stroke compared to executively intact patients (n=113). At the end of the 2-year follow-up period, the patients who were executively impaired at 6 months (n=53; 37\%) still performed worse than executively intact patients (n=92) in list learning (p{\textless}.001; partial Œ∑2=.096), and additionally in delayed recall of the list (p=.006; partial Œ∑2=.052) and immediate recall of geometric figures (p=.007; partial Œ∑2=.050).
{CONCLUSIONS}: In our working-aged stroke patients, executive impairment was common. Executive impairment was associated with memory tasks that provided less inherent structure and required the use of active memory strategies. Clinicians should remember this role of executive dysfunction when interpreting memory performance.},
	pages = {551--560},
	number = {5},
	journaltitle = {Journal of the International Neuropsychological Society: {JINS}},
	shortjournal = {J Int Neuropsychol Soc},
	author = {Turunen, Katri E. A. and Laari, Siiri P. K. and Kauranen, Tatu V. and Mustanoja, Satu and Tatlisumak, Turgut and Poutiainen, Erja},
	date = {2016-05},
	pmid = {27019324},
	keywords = {Memory Disorders, Cognition, Intelligence, Learning, Neuropsychology, Stroke, Cognition Disorders, Time Factors, Executive Function, Statistics, Nonparametric, Middle Aged, Aged, Female, Young Adult, Adult, Cohort Studies, Neuroimaging, Neurologic Examination, Humans, Adolescent, Neuropsychological Tests, Male, Cohort studies, Executive function, Memory, Episodic}
