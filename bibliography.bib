	date = {2007-01},
	pmid = {17158334},
	keywords = {Dementia, Predictive Value of Tests, Comorbidity, Prognosis, Stroke, Cognition Disorders, Longitudinal Studies, Middle Aged, Aged, Incidence, Female, Adult, Aged, 80 and over, Cohort Studies, Spain, Humans, Neuropsychological Tests, Follow-Up Studies, Male, Risk Factors, Surveys and Questionnaires},
