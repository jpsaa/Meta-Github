	number = {3},
	journaltitle = {Proceedings of Singapore Healthcare},
	shortjournal = {Proc. Singapore Healthc.},
	author = {Ng, Yee Sien and Astrid, Suantio and De Silva, Deidre Anne and Tan, May Leng Dawn and Tan, Yeow Leng and Chew, Effie},
	urldate = {2020-04-02},
	date = {2013-09-01},
	langid = {english},
