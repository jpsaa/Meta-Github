	journaltitle = {{BMC} complementary and alternative medicine},
	shortjournal = {{BMC} Complement Altern Med},
	author = {Wang, Shuhua and Yang, Hongling and Zhang, Jie and Zhang, Bin and Liu, Tao and Gan, Lu and Zheng, Jiangang},
